<!DOCTYPE html>
<html>
<head>
  <title>AddressNormalizer</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

    <%= render 'shared/nav' unless current_page?(:controller => 'welcome', :action => 'home')%>
    <div class="main-wrapper" style="min-height: 550px;">
      <%= yield %>
    </div>

    <%#= debug(params) if Rails.env.development? %>
    <%= render 'shared/footer' %>
</body>
</html>
